{
  "name": "fnp-vcarsenal",
  "displayName": "FNP VCARSENAL",
  "description": "VS Code extension to edit and preview license files with interactive controls, Opengrok support, Selective-picker commands support.",
  "version": "0.0.1",
  "publisher": "adas",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openPanel",
    "onStartupFinished",
    "onDidChangeActiveTextEditor",
    "onDidChangeTextEditorSelection",
    "workspaceContains:**/*.lic",
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openPanel",
        "title": "Open License Editor Panel",
        "category": "FNP VCArsenal"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "fnpVcarsenal",
          "title": "FNP VCArsenal",
          "icon": "media/fnpcode_logo.png"
        }
      ]
    },
    "views": {
      "fnpVcarsenal": [
        {
          "id": "licenseEditorView",
          "name": "FNP License Editor",
          "icon": "media/fnpcode_logo.png"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ."
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@types/vscode": "^1.70.0",
    "typescript": "^4.0.0",
    "vscode": "^1.1.37"
  }
}
